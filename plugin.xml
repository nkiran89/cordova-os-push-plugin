<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-os-push-plugin" version="1.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
<name>OSPushPlugin</name>
<js-module name="OSPushPlugin" src="www/OSPushPlugin.js">
<clobbers target="cordova.plugins.OSPushPlugin" />
</js-module>
<platform name="android">
<config-file parent="/*" target="res/xml/config.xml">
<feature name="OSPushPlugin">
<param name="android-package" value="com.example.ospushplugin.OSPushPlugin" />
</feature>
</config-file>
<config-file parent="/*" target="AndroidManifest.xml">
</config-file>
<source-file src="src/android/OSPushPlugin.java" target-dir="src/cordova-os-push-plugin/OSPushPlugin" />
</platform>
</plugin>